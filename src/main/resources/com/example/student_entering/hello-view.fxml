<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.TextField?>
<GridPane xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.example.student_entering.StController"
          alignment="center" hgap="10" vgap="10">
    <Label text="First Name:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="firstnameField" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="Last Name:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="lasttnameField" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="Address:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="address" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="Birthday:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="birthday" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="Mobile Number:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="mobilenumber" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="E-mail:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="emailaddress" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="Holder's Name:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="holdername" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label text="Holder's Mobile Number:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <TextField fx:id="holdersphonenumber" GridPane.rowIndex="0" GridPane.columnIndex="1"/>



    <Button text="Add Student" onAction="#addStudent" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
</GridPane>
